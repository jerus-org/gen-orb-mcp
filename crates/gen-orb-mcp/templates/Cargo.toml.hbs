[package]
name = "{{crate_name}}"
version = "{{version}}"
edition = "2021"
{{#if description}}
description = """
{{description}}
"""
{{/if}}

# Generated by gen-orb-mcp from the {{orb_name}} CircleCI orb

[dependencies]
rmcp = { version = "0.14", features = ["server", "transport-io"] }
tokio = { version = "1", features = ["rt-multi-thread", "macros"] }
anyhow = "1.0"
tracing-subscriber = { version = "0.3", features = ["env-filter"] }
serde_json = "1.0"

[[bin]]
name = "{{crate_name}}"
path = "src/main.rs"

[lib]
name = "{{crate_name}}"
path = "src/lib.rs"
